{
  "name": "stockreport",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon server.js",
    "seed": "cross-env NODE_ENV=development sequelize db:seed:all",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,css,less,ejs,scss,sass}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomas-claireau/stockreport.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thomas-claireau/stockreport/issues"
  },
  "homepage": "https://github.com/thomas-claireau/stockreport#readme",
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "lint-staged": "^12.3.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^15.0.0",
    "express": "^4.17.2",
    "express-rate-limit": "^6.2.0",
    "helmet": "^5.0.2",
    "hpp": "^0.2.3",
    "mysql2": "^2.3.3",
    "puppeteer": "^13.1.3",
    "sequelize": "^6.15.1"
  }
}
